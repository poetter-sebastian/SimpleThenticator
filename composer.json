{
    "name": "sebastiandevs/simplethenticator",
    "description": "TOTP based on https://github.com/PHPGangsta/GoogleAuthenticator updated and reworked to php8.2 because of inactivity of the original creator.",
    "keywords": ["TOTP","2FA", "2 factor authenticator library"],
    "homepage": "https://poetter-sebastian.github.io/",
    "version": "1.1",
    "minimum-stability": "stable",
    "license": "BSD-2-Clause",
    "authors": [
        {
            "name": "Sebastian Poetter"
        }
    ],
    "support": {
        "source": "https://github.com/poetter-sebastian/SimpleThenticator",
        "issues": "https://github.com/poetter-sebastian/SimpleThenticator/issues"
    },
    "config": {
        "optimize-autoloader": true
    },
    "require": {
        "php": ">=8.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.5.36"
    },
    "autoload": {
        "classmap": [
            "src/"
        ],
        "psr-4": {
            "src\\": "src/SimpleAuthenticator.php"
        }
    },
    "scripts": {
        "build": "composer install --no-dev",
        "build-dev": "composer install",
        "test": "phpunit --colors=always --configuration=phpunit.xml"
    }
}
